{
    "version": 1,
    "disable_existing_loggers": false,
    "handlers": {
        "console": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "debug.log"
        }
    },
    "loggers": {
        "qabel_provider": {
            "handlers": ["console"],
            "level": "DEBUG",
            "propagate": true
        },
        "django_prometheus": {
            "handlers": ["console"],
            "level": "DEBUG",
            "propagate": true
        }
    }
}
